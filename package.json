{
  "name": "react-image",
  "version": "1.0.0",
  "description": "about image react component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/react-image.js",
  "author": "andrew",
  "license": "MIT",
  "scripts": {
    "dev": "rollup -c -w -o dist/react-image.js",
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "coverage": "yarn lint",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib --extensions '.ts,.tsx'",
    "build:es": "cross-env babel src --out-dir es --extensions '.ts,.tsx'",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/react-image.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/react-image.min.js",
    "build:ts:es": "tsc -p tsconfig.es.json",
    "build:ts:lib": "tsc -p tsconfig.lib.json",
    "build": "run-s build:* build:*:*",
    "prebuild": "yarn clean",
    "pretest": "yarn lint && yarn build",
    "prepare": "yarn test",
    "prepublishOnly": "cd ../demo && yarn deploy",
    "storybook": "start-storybook -s ./static -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "yarn build-storybook && storybook-to-ghpages -- --existing-output-dir=dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-react-jsx": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/plugin-transform-typescript": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@rollup/plugin-replace": "^2.3.2",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@storybook/storybook-deployer": "^2.8.6",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.12",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest": "^26.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0"
  },
  "dependencies": {
    "intersection-observer": "^0.10.0",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
